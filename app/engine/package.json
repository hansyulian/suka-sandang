{
  "name": "@app/engine",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "dev": "tsc --build --watch",
    "build": "tsc --build tsconfig.build.json",
    "postbuild": "tsc-alias -p tsconfig.cjs.json --outDir dist/cjs && tsc-alias -p tsconfig.esm.json --outDir dist/esm",
    "clean": "shx mkdir -p dist && shx rm -rf dist",
    "migrate": "npx sequelize db:migrate",
    "migrate:reset": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate",
    "prebuild": "pnpm run clean",
    "watch": "npx nodemon",
    "test": "npx jest"
  },
  "type": "commonjs",
  "sideEffects": false,
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    "import": "./dist/esm/src/index.js",
    "require": "./dist/cjs/src/index.js",
    "types": "./dist/types/src/index.d.ts"
  },
  "files": [
    "dist",
    "src",
    "LICENSE"
  ],
  "dependencies": {
    "@hyulian/api-contract": "workspace:*",
    "@hyulian/common": "workspace:*",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "sequelize-cli": "^6.6.2",
    "shx": "^0.3.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
